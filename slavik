##### f time
function time()
{
sleep 2
clear
echo "------------------"
sleep 2
clear
echo "----------------------------------"
sleep 2
clear
echo "--------------------------------------------------------------"
sleep 2
clear
echo -e " COMPLETE "
sleep 2
clear
pwd
ls
}

